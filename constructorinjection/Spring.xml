<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN 2.0//EN" 
	"http://www.springframework.org/dtd/spring-beans-2.0.dtd">
<beans>
	<bean id="s" class="constructorinjection.student">
		<constructor-arg>
			<list>
				<value>java</value>
				<value>python</value>
			</list>
		</constructor-arg>

		<constructor-arg>
			<set>
				<value>Angular</value>
				<value>React</value>
			</set>
		</constructor-arg>

		<constructor-arg>
			<map>
				<entry key="101" value="web"></entry>
				<entry key="102" value="micro"></entry>
			</map>
		</constructor-arg>

	</bean>
</beans>